CREATE TABLE exercise_logs
    (id INTEGER PRIMARY KEY AUTOINCREMENT,
    type TEXT,
    minutes INTEGER, 
    calories INTEGER,
    heart_rate INTEGER);


INSERT INTO exercise_logs(type, minutes, calories, heart_rate) VALUES ("running", 30, 210, 130);
INSERT INTO exercise_logs(type, minutes, calories, heart_rate) VALUES ("walking", 10, 30, 105);
INSERT INTO exercise_logs(type, minutes, calories, heart_rate) VALUES ("dancing", 15, 200, 120);


SELECT * FROM exercise_logs WHERE calories > 50 AND minutes < 30;         

id	type	     minutes	calories	heart_rate
3	walking	        10	       30	        105


SELECT * FROM exercise_logs WHERE calories > 50 OR heart_rate > 100;      

id	type	     minutes	calories	heart_rate
1	running	        30	       210      	130
2	walking	        10	        30	        105
3	dancing	        15	       200	        120
