#print to string

import pandas as pd

df = pd.read_csv('https://raw.githubusercontent.com/deartc/FitBitLit/main/data.csv')

df['Calories'] = pd.to_datetime(df['Calories'])

print(df.to_string())






#visualize  scatter
import pandas as pd
import matplotlib.pyplot as plt

df = pd.read_csv'(https://raw.githubusercontent.com/deartc/FitBitLit/main/data.csv')

df.plot(kind = 'scatter', x = 'Duration', y = 'Calories')

plt.show()
